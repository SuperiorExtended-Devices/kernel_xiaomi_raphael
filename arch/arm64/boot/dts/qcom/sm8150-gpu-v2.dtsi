&soc {
	gpu_opp_table_v2: gpu_opp_table_v2 {
		compatible = "operating-points-v2";
		opp-810000000 {
			opp-hz = /bits/ 64 <767000000>;
			opp-microvolt = <284>;
		};

		opp-765000000 {
			opp-hz = /bits/ 64 <725000000>;
			opp-microvolt = <202>;
		};

		opp-675000000 {
			opp-hz = /bits/ 64 <620000000>;
			opp-microvolt = <138>;
		};

		opp-585000000 {
			opp-hz = /bits/ 64 <535000000>;
			opp-microvolt = <76>;
		};

		opp-499200000 {
			opp-hz = /bits/ 64 <480000000>;
			opp-microvolt = <69>;
		};

		opp-427000000 {
			opp-hz = /bits/ 64 <360000000>;
			opp-microvolt = <23>;
		};

		opp-345000000 {
			opp-hz = /bits/ 64 <270000000>;
			opp-microvolt = <22>;
		};

		opp-257000000 {
			opp-hz = /bits/ 64 <180000000>;
			opp-microvolt = <21>;
		};
	};
};
